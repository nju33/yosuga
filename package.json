  "scripts": {
    "build": "yarn build:prod && yarn flow-copy-source",
    "build:prod": "NODE_ENV=production gulp lib",
    "build:docs": "node -r babel-register build-demo.js",
    "flow-copy-source": "flow-copy-source lib dist -i '*.test.js' -i 'view/**'",
    "dev": "NODE_ENV=dev gulp watch",
    "dev:browser": "NODE_ENV=dev gulp watch:browser",
    "flow-typed": "flow-typed install -f v`cat package.json | grep \\\"flow-bin\\\" | tr -cd [0-9].`",
    "prepare": "yarn build",
    "test": "jest lib/**",
    "test:ci": "yarn test -- --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  }
